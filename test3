def task17():
    """
    Наименьшее N, чтобы результат > 99 (дважды добавляем остаток).
    """
    def process(N):
        binary = bin(N)[2:]
        
        # Первое добавление
        ones_count = binary.count('1')
        binary += str(ones_count % 2)
        
        # Второе добавление (на новой записи)
        ones_count = binary.count('1')
        binary += str(ones_count % 2)
        
        return int(binary, 2)
    
    for N in range(1, 1000):
        R = process(N)
        if R > 99:
            return N
    return None

def task18():
    """
    Минимальное R > 45, которое может быть результатом алгоритма.
    """
    def process(N):
        binary = bin(N)[2:]
        
        # Первое добавление
        ones_count = binary.count('1')
        binary += str(ones_count % 2)
        
        # Второе добавление (на новой записи)
        ones_count = binary.count('1')
        binary += str(ones_count % 2)
        
        return int(binary, 2)
    
    results = []
    for N in range(1, 1000):
        R = process(N)
        if R > 45:
            results.append(R)
    
    return min(results) if results else None

def task19():
    """
    Максимальное R ≤ 57, которое может быть результатом алгоритма.
    """
    def process(N):
        binary = bin(N)[2:]
        
        # Первое добавление
        ones_count = binary.count('1')
        binary += str(ones_count % 2)
        
        # Второе добавление (на новой записи)
        ones_count = binary.count('1')
        binary += str(ones_count % 2)
        
        return int(binary, 2)
    
    results = []
    for N in range(1, 1000):
        R = process(N)
        if R <= 57:
            results.append(R)
    
    return max(results) if results else None

