# ================== ЗАДАНИЕ 7 ==================
def task7():
    """Наибольшее трёхзначное число, дающее результат 33621"""
    def process(num):
        a, b, c = map(int, str(num))
        product = a * b * c
        sum_digits = a + b + c
        
        if product >= sum_digits:
            return int(str(product) + str(sum_digits))
        else:
            return int(str(sum_digits) + str(product))
    
    result = 33621
    max_num = 0
    for num in range(100, 1000):
        if process(num) == result:
            if num > max_num:
                max_num = num
    return max_num

