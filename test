# ================== ЗАДАНИЕ 2 ==================
def task2():
    """Максимальное N, дающее R <= 37"""
    def process(n):
        # Шаг 1
        binary = bin(n)[2:]
        
        # Шаг 2
        if n % 2 == 0:
            binary += '0'
        else:
            binary += '1'
        
        # Шаг 3
        ones_count = binary.count('1')
        if ones_count % 3 == 0:
            binary = '11' + binary[2:]
        else:
            binary = '10' + binary[2:]
        
        return int(binary, 2)
    
    max_n = 0
    for n in range(1, 1000):
        r = process(n)
        if r <= 37:
            if n > max_n:
                max_n = n
    return max_n

# ================== ЗАДАНИЕ 3 ==================
def task3():
    """Минимальное N, дающее R >= 26"""
    def process(n):
        binary = bin(n)[2:]
        
        if len(binary) % 2 == 0:
            mid = len(binary) // 2
            binary = binary[:mid] + '1' + binary[mid:]
        
        return int(binary, 2)
    
    for n in range(1, 1000):
        r = process(n)
        if r >= 26:
            return n
    return None

# ================== ЗАДАНИЕ 4 ==================
def task4():
    """Максимальное N, дающее R <= 26"""
    def process(n):
        binary = bin(n)[2:]
        
        if len(binary) % 2 == 0:
            mid = len(binary) // 2
            binary = binary[:mid] + '11' + binary[mid:]
        
        return int(binary, 2)
    
    max_n = 0
    for n in range(1, 1000):
        r = process(n)
        if r <= 26:
            if n > max_n:
                max_n = n
    return max_n

# ================== ЗАДАНИЕ 5 ==================
def task5():
    """Наибольшее трёхзначное число, дающее результат 9752"""
    def process(num):
        a, b, c = map(int, str(num))
        sum1 = a*a + b*b
        sum2 = b*b + c*c
        if sum1 >= sum2:
            return int(str(sum1) + str(sum2))
        else:
            return int(str(sum2) + str(sum1))
    
    result = 9752
    max_num = 0
    for num in range(100, 1000):
        if process(num) == result:
            if num > max_num:
                max_num = num
    return max_num

