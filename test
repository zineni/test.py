# ================== ЗАДАНИЕ 11 ==================
def task11():
    """Наибольшее N, дающее R < 64"""
    def process(n):
        # Шаг 1
        step1 = n - (n % 4)
        
        # Шаг 2
        binary = bin(step1)[2:]
        
        # Шаг 3
        sum_digits = sum(int(d) for d in binary)
        binary += str(sum_digits % 2)
        
        sum_digits = sum(int(d) for d in binary)
        binary += str(sum_digits % 2)
        
        return int(binary, 2)
    
    max_n = 0
    for n in range(1, 1000):
        r = process(n)
        if r < 64:
            if n > max_n:
                max_n = n
    return max_n

# ================== ЗАДАНИЕ 12 ==================
def task12():
