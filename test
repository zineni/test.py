        if remainder == 0:
            binary += '00'
        elif remainder == 1:
            binary += '01'
        elif remainder == 2:
            binary += '10'
        else:  # remainder == 3
            binary += '11'
        
        return int(binary, 2)
    
    max_r = 0
    for n in range(1, 1000):
        r = process(n)
        if r < 111 and r > max_r:
            max_r = r
    return max_r

