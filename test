# ================== ЗАДАНИЕ 13 ==================
def task13():
    """Минимальное R > 90, которое может быть результатом"""
    def process(n):
        # Шаг 1
        step1 = n - (n % 8) + (n % 2)
        
        # Шаг 2
        binary = bin(step1)[2:]
        
        # Шаг 3
        sum_digits = sum(int(d) for d in binary)
        binary += str(sum_digits % 2)
        
        sum_digits = sum(int(d) for d in binary)
        binary += str(sum_digits % 2)
        
        return int(binary, 2)
    
    min_r = float('inf')
    for n in range(1, 1000):
        r = process(n)
        if r > 90 and r < min_r:
            min_r = r
    return min_r

# ================== ЗАДАНИЕ 14 ==================
def task14():
    """Минимальное R > 97, которое может быть результатом"""
    def process(n):
        # Шаг 1
        step1 = n - (n % 8) + (n % 2)
        
        # Шаг 2
        binary = bin(step1)[2:]
        
        # Шаг 3
        sum_digits = sum(int(d) for d in binary)
        binary += str(sum_digits % 2)
        
        sum_digits = sum(int(d) for d in binary)
        binary += str(sum_digits % 2)
        
        return int(binary, 2)
    
    min_r = float('inf')
    for n in range(1, 1000):
        r = process(n)
        if r > 97 and r < min_r:
            min_r = r
    return min_r

# ================== ЗАДАНИЕ 18 ==================
def task18():
    """Максимальное нечётное R < 256"""
    def process(n):
        binary_n = bin(n)[2:]
        result_binary = ''
        
        for digit in binary_n:
            if digit == '0':
                result_binary += '01'
            else:  # digit == '1'
                result_binary += '10'
        
        return int(result_binary, 2)
    
    max_r = 0
    for n in range(1, 1000):
        r = process(n)
        if r < 256 and r % 2 == 1:
            if r > max_r:
                max_r = r
    return max_r

# ================== ЗАДАНИЕ 19 ==================
def task19():
    """Максимальное R < 100"""
    def process(n):
        binary = bin(n)[2:]
        
        remainder = n % 4
        if remainder == 0:
            binary += '00'
        elif remainder == 1:
            binary += '01'
        elif remainder == 2:
            binary += '10'
        else:  # remainder == 3
            binary += '11'
        
        return int(binary, 2)
    
    max_r = 0
    for n in range(1, 1000):
        r = process(n)
        if r < 100 and r > max_r:
            max_r = r
    return max_r

# ================== ЗАДАНИЕ 20 ==================
def task20():
    """Максимальное R < 111"""
    def process(n):
        binary = bin(n)[2:]
        
