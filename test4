def task1():

    def count_combinations(pos1, pos2):
        other_positions = [i for i in range(5) if i not in (pos1, pos2)]
        return 3 ** len(other_positions)
    
    total = 0
    for i in range(5):
        for j in range(i+1, 5):
            total += count_combinations(i, j)
    return total

def task2():

    from itertools import permutations
    
    digits = [0,2,3,4,5,6,7]
    count = 0
    
    for p in permutations(digits, 5):
        if p[0] == 0:
            continue
        
        valid = True
        for i in range(4):
            if (p[i] % 2 == 0 and p[i+1] % 2 == 0) or \
               (p[i] % 2 == 1 and p[i+1] % 2 == 1):
                valid = False
                break
        if valid:
            count += 1
    return count

def task3():

    count = 0
    for num in range(9**4, 9**5):  
        digits = []
        n = num
        for _ in range(5):
            digits.append(n % 9)
            n //= 9
        digits.reverse()
        
        if digits[0] == 0:
            continue
        
        if digits.count(5) != 1:
            continue
        
        pos = digits.index(5)
        
        valid = True
        if pos > 0 and digits[pos-1] % 2 == 1:
            valid = False
        if pos < 4 and digits[pos+1] % 2 == 1:
            valid = False
        
        if valid:
            count += 1
    return count

def task4():

    count = 0
    for num in range(1000, 10000):
        if num % 5 != 0:
            continue
        
        digits = [int(d) for d in str(num)]
        if len(set(digits)) == len(digits):
            count += 1
    return count

def task5():

    count = 0
    for num in range(1000, 10000):
        digits = [int(d) for d in str(num)]
        
        if len(set(digits)) != len(digits):
            continue
        
        valid = True
        for i in range(3):
            if (digits[i] % 2 == 0 and digits[i+1] % 2 == 0) or \
               (digits[i] % 2 == 1 and digits[i+1] % 2 == 1):
                valid = False
                break
        
        if valid:
            count += 1
    return count

def task6():

    count = 0
    for num in range(6**4, 6**5):
        digits = []
        n = num
        for _ in range(5):
            digits.append(n % 6)
            n //= 6
        digits.reverse()
        
        if digits.count(5) < 2:
            continue
        
        odd_less_than_4 = sum(1 for d in digits if d in (1, 3))
        if odd_less_than_4 > 3:
            continue
        
        count += 1
    return count

def task7():

    count = 0
    for d1 in range(1, 7): 
        for d2 in range(d1):
            for d3 in range(d2):
                for d4 in range(d3):
                    count += 1
    return count

def task8():

    def count_combinations(positions):
        other_positions = [i for i in range(5) if i not in positions]
        return 4 ** len(other_positions)
    
    total = 0
    for i in range(5):
        for j in range(i+1, 5):
            for k in range(j+1, 5):
                total += count_combinations((i, j, k))
    return total

def task9():

    return task5()

def task10():

    alphabet = ['А', 'Г', 'И', 'Л', 'М', 'О', 'Р', 'Т']
    letter_to_num = {letter: i for i, letter in enumerate(alphabet)}
    
    count = 0
    word_num = 1
    
    for i1 in range(8):
        for i2 in range(8):
            for i3 in range(8):
                for i4 in range(8):
                    for i5 in range(8):
                        if word_num % 2 == 1:
                            word = [alphabet[i1], alphabet[i2], alphabet[i3], 
                                   alphabet[i4], alphabet[i5]]
                            if word[0] != 'Г':
                                if word.count('И') >= 2:
                                    count += 1
                        word_num += 1
    return count

def task11():

    count = 0
    for d1 in range(1, 4):  
        for d2 in range(4):
            for d3 in range(4):
                if d1 + d3 > d2:
                    count += 1
    return count

def task12():
    from math import comb

    total = 0
    for k in range(0, 5):
        total += comb(6, k) * (5**k) * (9**(6-k))

    result = comb(7, 2) * total
    return result
    
def task13():

    count = 0
    first_digits = [0,2,4,6]
    
    for num in range(7**4, 7**5):
        digits = []
        n = num
        for _ in range(5):
            digits.append(n % 7)
            n //= 7
        digits.reverse()
        
        if digits[0] not in first_digits:
            continue
        
        if digits[4] in (2, 3):
            continue
        
        if digits.count(1) < 2:
            continue
        
        count += 1
    return count

def task14():
    count = 0
    for num in range(9**4, 9**5):
        digits = []
        n = num
        for _ in range(5):
            digits.append(n % 9)
            n //= 9
        digits.reverse()
        
        if digits.count(3) != 2:
            continue
        
        valid = True
        for i in range(5):
            if digits[i] == 2:
                if i > 0 and digits[i-1] % 2 == 1:
                    valid = False
                    break
                if i < 4 and digits[i+1] % 2 == 1:
                    valid = False
                    break
        
        if valid:
            count += 1
    return count

def task15():

    letters = ['М','А','Н','Г','Х','С','Т']
    count = 0
    
    for i1 in range(7):
        if letters[i1] == 'А':
            continue
        for i2 in range(7):
            for i3 in range(7):
                for i4 in range(7):
                    for i5 in range(7):
                        for i6 in range(7):
                            word = [letters[i1], letters[i2], letters[i3],
                                   letters[i4], letters[i5], letters[i6]]
                            if word.count('М') != 2:
                                continue
                            if word.count('А') < 1:
                                continue
                            count += 1
    return count

def task16():
    letters = ['К','О','Н','Ф','Е','Т','А']
    count = 0
    
    for i1 in range(7):
        for i2 in range(7):
            if letters[i2] == 'Ф':
                continue
            for i3 in range(7):
                for i4 in range(7):
                    for i5 in range(7):
                        word = [letters[i1], letters[i2], letters[i3],
                               letters[i4], letters[i5]]
                        if word.count('К') == 3:
                            count += 1
    return count

def task17():
    from itertools import permutations
    
    letters = ['М','У','Ж','Ч','И','Н','А']
    count = 0
    word_num = 1
    
    for perm in permutations(letters, 6):
        if perm[0] == 'Ч':
            word_num += 1
            continue
        
        if 'Ж' not in perm:
            word_num += 1
            continue
        
        if word_num % 2 == 1:
            count += 1
        
        word_num += 1
    
    return count

def main():
    print("Задача 1:", task1())
    print("Задача 2:", task2())
    print("Задача 3:", task3())
    print("Задача 4:", task4())
    print("Задача 5:", task5())
    print("Задача 6:", task6())
    print("Задача 7:", task7())
    print("Задача 8:", task8())
    print("Задача 9:", task9())
    print("Задача 10:", task10())
    print("Задача 11:", task11())
    print("Задача 12:", task12())
    print("Задача 13:", task13())
    print("Задача 14:", task14())
    print("Задача 15:", task15())
    print("Задача 16:", task16())
    print("Задача 17:", task17())

if __name__ == "__main__":
    main()
